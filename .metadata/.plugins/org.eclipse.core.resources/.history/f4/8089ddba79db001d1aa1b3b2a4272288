package modelDB;

public class Person {

	String ID;
	String Name;
	String Username;
	byte[] Password;

	public Person() {
		super();
	}

	public Person(String ID, String Name, String Surname, String Phone, String Address, String Username,
			byte[] Password) {
		this.ID = ID;
		this.Name = Name;
		this.Username = Username;
		this.Password = Password;
	}

	public String getID() {
		return ID;
	}

	public void setID(String iD) {
		ID = iD;
	}

	public String getName() {
		return Name;
	}

	public void setName(String name) {
		Name = name;
	}

	public String getUsername() {
		return Username;
	}

	public void setUsername(String username) {
		Username = username;
	}

	public byte[] getPassword() {
		return Password;
	}

	public void setPassword(byte[] password) {
		Password = password;
	}

}
